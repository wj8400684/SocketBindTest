{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://*:8081"
      },
      "desktop": {
        "Url": "http://*:9090",
        "ForwardPort": 3389,
        "ForwardAddress": "127.0.0.1"
      }
    }
  }
}

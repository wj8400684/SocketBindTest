{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://*:8080"
      },
      "desktop": {
        "Url": "http://*:9090",
        "ForwardPort": 443,
        "ForwardAddress": "www.ip138.com",
        "IsForward": true
      },
      "telnet": {
        "Url": "http://*:8889",
        "IsForward": false
      }
    }
  }
}

{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080"
      },
      "Telnet": {
        "Url": "http://*:9000"
      },
      "Forward": {
        "Url": "http://*:9090"
      }
    }
  },
  "SocketTransportOptions": {
    "Backlog": 65535,
    "UnsafePreferInlineScheduling": true
  }
}
